<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout android:id="@+id/activity_demo"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:swrv="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yougourta.projmob.NavDrawer.ConfirmerRdvs"
    android:background="#e9ebee">

    <include
        layout="@layout/toolbar_dyalna"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/include"
        android:layout_marginRight="0dp"
        swrv:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="0dp"
        swrv:layout_constraintLeft_toLeftOf="parent"
        swrv:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp"
        swrv:layout_constraintHorizontal_bias="0.0" />

    <io.huannguyen.swipeablerv.view.SWRecyclerView
        android:id="@+id/recycler_view"
        android:scrollbars="vertical"
        android:layout_width="0dp"
        android:layout_height="0dp"
        swrv:border_color="@android:color/darker_gray"
        swrv:ltr_swipe_background="#F44336"
        swrv:rtl_swipe_background="#1cbe9d"
        swrv:swipe_message_text_size="16sp"
        swrv:swipe_message_color="#fff"
        swrv:swipe_message_bold="true"
        swrv:swipe_icon_color="#FFFFFF"
        swrv:swipe_icon_height="24dp"
        swrv:swipe_icon_width="24dp"
        swrv:swipe_icon_margin="24dp"
        swrv:ltr_snackbar_message="Annuler"
        swrv:rtl_snackbar_message="Annuler"
        swrv:snackbar_message_color="#fff"
        swrv:ltr_snackbar_background="#75757575"
        swrv:rtl_snackbar_background="#75757575"
        swrv:undo_action_text_color="#fff"
        swrv:has_border="true"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        swrv:layout_constraintBottom_toBottomOf="parent"
        swrv:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="0dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        swrv:layout_constraintLeft_toLeftOf="parent"
        swrv:layout_constraintVertical_bias="0.0"
        swrv:layout_constraintTop_toBottomOf="@+id/include" />

</android.support.constraint.ConstraintLayout>